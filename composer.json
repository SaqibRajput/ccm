{
    "name": "ccm/leads",
    "description": "Functions and settings for all services",
    "keywords": [
        "ccm",
        "leads"
    ],
    "homepage": "https://github.com/ccm/leads",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Saqib Rajput",
            "email": "saqib.rajput@arpatech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "CCM\\Leads\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCM\\Leads\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CCM\\Leads\\LeadsServiceProvider"
            ],
            "aliases": {
                "Leads": "CCM\\Leads\\LeadsFacade"
            }
        }
    }
}
